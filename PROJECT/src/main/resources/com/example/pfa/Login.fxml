            <Label layoutX="34.0" layoutY="185.0" text="Password" />
            <TextField fx:id="txtPass" layoutX="34.0" layoutY="211.0" />
            <Text layoutX="32.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="LOGIN">
               <font>
                  <Font name="Arial Bold" size="26.0" />
               </font>
            </Text>
            <Button layoutX="34.0" layoutY="251.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="26.0" prefWidth="150.0" text="Login" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
